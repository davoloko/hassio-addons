{
  "name": "Amule",
  "version": "0.0.0",
  "slug": "Amule",
  "description": "aMule add-on",
  "url": "https://github.com/davoloko/hassio-addons/tree/master/amule",
  "apparmor": true,
  "arch": [
    "armhf",
    "armv7",
    "aarch64",
    "amd64",
    "i386"
  ],
  "boot": "auto",
  "upstream": "0.0.8",
  "webui": "http://[HOST]:[PORT:4711]",
  "startup": "services",
  "backup_exclude": [
    "**/Backups/*",
    "**/logs/*",
    "**/MediaCover/*"
  ],
  "devices": [
    "/dev/sda",
    "/dev/sdb",
    "/dev/sdc",
    "/dev/sdd",
    "/dev/sde",
    "/dev/sdf",
    "/dev/sdg",
    "/dev/nvme",
    "/dev/mmcblk",
    "/dev/sda1",
    "/dev/sdb1",
    "/dev/sdc1",
    "/dev/sdd1",
    "/dev/sde1",
    "/dev/sdf1",
    "/dev/sdg1",
    "/dev/sda2",
    "/dev/sdb2",
    "/dev/sdc2",
    "/dev/sdd2",
    "/dev/sde2",
    "/dev/sdf2",
    "/dev/sdg2",
    "/dev/sda3",
    "/dev/sdb3",
    "/dev/sda4",
    "/dev/sdb4",
    "/dev/nvme0",
    "/dev/nvme1",
    "/dev/nvme2"
  ],
  "environment": {
    "PUID": "1000",
    "PGID": "1000",
    "TZ": "Europe/Madrid",
    "GUI_PWD": "<fill_password>",
    "WEBUI_PWD": "<fill_password>",
    "MOD_AUTO_RESTART_ENABLED": "true",
    "MOD_AUTO_RESTART_CRON": "0 6 * * *",
    "MOD_AUTO_SHARE_ENABLED": "false",
    "MOD_AUTO_SHARE_DIRECTORIES": "/incoming;/my_movies",
    "MOD_FIX_KAD_GRAPH_ENABLED": "true"
  },
  "ingress": false,
  "init": false,
  "map": [
    "config:rw",
    "share:rw",
    "media:rw",
    "ssl"
  ],
  "options": {
    "PUID": 0,
    "PGID": 0,
    "TZ": "Europe/Madrid",
    "GUI_PWD": "<fill_password>"
  },
  "ports": {
    "4711/tcp": 4711,
    "4712/tcp": 4712,
    "4662/tcp": 4662,
    "4665/udp": 4665,
    "4672/udp": 4672
  },
  "ports_description": {
    "4711": "web ui",
    "4712": "remote gui, webserver, cmd ...",
    "4662": "ed2k tcp",
    "4665/udp": "ed2k global search udp (tcp port +3)",
    "4672/udp": "ed2k udp"
  },
  "privileged": [
    "SYS_ADMIN",
    "DAC_READ_SEARCH"
  ],
  "schema": {
    "PUID": "int",
    "PGID": "int",
    "TZ": "str?",
    "networkdisks": "str?",
    "cifsusername": "str?",
    "cifspassword": "str?"
  }
}
